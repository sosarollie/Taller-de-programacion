programa TP1EJ5
procesos
  proceso juntarFlor
  comenzar
    mientras HayFlorEnLaEsquina
      tomarFlor
  fin
  
  proceso juntarPapel
  comenzar
    mientras HayPapelEnLaEsquina
      tomarPapel
  fin
  
  proceso recorrerCallePapel
  comenzar
    juntarPapel
    repetir 19
      mover
      juntarPapel
  fin
  
  proceso recorrerCalleFlor
  comenzar
    juntarFlor
    repetir 19
      mover
      juntarFlor
  fin  
areas
  area1: AreaP(1,75,20,80)
  area2: AreaP(75,81,80,100)
  area3: AreaP(81,10,100,15)
  area4: AreaP(10,1,15,20)
robots
  robot tipoFlor
  comenzar
    derecha
    recorrerCalleFlor
    mientras HayFlorEnLaBolsa
      depositarFlor
    Pos(PosAv - 19,PosCa + 5)
    recorrerCalleFlor
    mientras HayFlorEnLaBolsa
      depositarFlor
  fin
  
  robot tipoPapel
  comenzar
    recorrerCallePapel
    mientras HayPapelEnLaBolsa
      depositarPapel
    Pos(PosAv + 5,PosCa - 19)
    recorrerCallePapel
    mientras HayPapelEnLaBolsa
      depositarPapel
  fin
variables
  r1: tipoFlor
  r2: tipoPapel
  r3: tipoFlor
  r4: tipoPapel
comenzar
  AsignarArea(r1,area1)
  AsignarArea(r2,area2)
  AsignarArea(r3,area3)
  AsignarArea(r4,area4)
  Iniciar(r1,1,75)
  Iniciar(r2,75,81)
  Iniciar(r3,81,10)
  Iniciar(r4,10,1)
fin
